{
  "name": "audunru/social-accounts",
  "description": "Add social login to your Laravel app",
  "keywords": [
    "socialite",
    "laravel",
    "oauth"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Audun Rundberg",
      "email": "audun@rundberg.no"
    }
  ],
  "require": {
    "php": "^7.1",
    "doctrine/dbal": "^2.10",
    "illuminate/auth": "v7.x-dev",
    "illuminate/contracts": "v7.x-dev",
    "illuminate/database": "v7.x-dev",
    "illuminate/http": "v7.x-dev",
    "illuminate/support": "v7.x-dev",
    "laravel/socialite": "^3.0|^4.0"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.3",
    "orchestra/database": "^3.5.0|^3.6.0|^3.7.0|^3.8.0|^4.0|dev-master",
    "orchestra/testbench": "^3.5.0|^3.6.0|^3.7.0|^3.8.0|^4.0|dev-master",
    "php-coveralls/php-coveralls": "^2.2",
    "phpunit/phpunit": "^6.5.0|^7.5.0|^8.5.0"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "audunru\\SocialAccounts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "audunru\\SocialAccounts\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "audunru\\SocialAccounts\\SocialAccountsServiceProvider"
      ],
      "aliases": {
        "SocialAccounts": "audunru\\SocialAccounts\\Facades\\SocialAccounts"
      }
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpunit"
    ],
    "test-with-coverage": [
      "vendor/bin/phpunit --coverage-clover build/logs/clover.xml"
    ]
  }
}
